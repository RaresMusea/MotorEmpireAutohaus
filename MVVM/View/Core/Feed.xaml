<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MVVM.View.Core.Feed"
             xmlns:vehicles="clr-namespace:MVVM.View_Models.Vehicles"
             x:DataType="vehicles:SearchResultsViewModel"
             Title="Feed">
    <VerticalStackLayout>
        <Label 
            Margin="40"
            HorizontalTextAlignment="Center"
            FontSize="25"
            Text="{Binding CarFilter.ModelName}"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
    </VerticalStackLayout>
</ContentPage>